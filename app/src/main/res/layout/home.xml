<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/bgbg"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:gravity="left|center_vertical"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/linear41"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:paddingLeft="10dp"
			android:paddingTop="8dp"
			android:paddingBottom="8dp"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear42"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="60dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview7"
					android:layout_width="60dp"
					android:layout_height="60dp"
					android:src="@drawable/wolf1"
					android:scaleType="fitCenter" />
				<TextView
					android:id="@+id/textview3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="8dp"
					android:text="Wolf Books"
					android:textSize="25sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear51"
				android:layout_width="50dp"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="1" />
			<LinearLayout
				android:id="@+id/linear43"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear44"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview8"
						android:layout_width="33dp"
						android:layout_height="33dp"
						android:src="@drawable/ic_laptop_mac_white"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview4"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="Books"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear45"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview9"
						android:layout_width="33dp"
						android:layout_height="33dp"
						android:src="@drawable/cloud"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="Cloud"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear46"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview10"
						android:layout_width="33dp"
						android:layout_height="33dp"
						android:src="@drawable/calendar"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="Calendar"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear47"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview11"
						android:layout_width="33dp"
						android:layout_height="33dp"
						android:src="@drawable/location"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview7"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="Location"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear53"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview14"
						android:layout_width="33dp"
						android:layout_height="33dp"
						android:src="@drawable/setting"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview11"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="Setting"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear52"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview13"
						android:layout_width="33dp"
						android:layout_height="33dp"
						android:src="@drawable/ic_favorite_outline_white"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview10"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="Logout"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear48"
				android:layout_width="50dp"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="3" />
			<LinearLayout
				android:id="@+id/linear49"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear50"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="20dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview9"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Gives You Better Than Best"
						android:textSize="16sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/bg"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:background="#F4F4FA"
			android:gravity="center_horizontal"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/top"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/drawer"
					android:layout_width="30dp"
					android:layout_height="35dp"
					android:layout_marginTop="10dp"
					android:src="@drawable/menu"
					android:scaleType="fitCenter" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/mid"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:orientation="vertical">
				<TextView
					android:id="@+id/textview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Eceian"
					android:textSize="40sp"
					android:textColor="#15096F" />
				<TextView
					android:id="@+id/textview2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Attitude amplified, faults rectified..!!"
					android:textSize="16sp"
					android:textColor="#59549A" />
				<LinearLayout
					android:id="@+id/linear1"
					android:layout_width="match_parent"
					android:layout_height="62dp"
					android:layout_marginLeft="10dp"
					android:layout_marginTop="20dp"
					android:layout_marginRight="10dp"
					android:layout_marginBottom="35dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<EditText
						android:id="@+id/edittext1"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5dp"
						android:padding="8dp"
						android:background="#FFFFFF"
						android:textSize="16sp"
						android:textColor="#59549A"
						android:hint="Search"
						android:textColorHint="#59549A"
						android:layout_weight="1" />
					<LinearLayout
						android:id="@+id/linear2"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:layout_marginLeft="5dp"
						android:layout_marginRight="5dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<ImageView
							android:id="@+id/imageview2"
							android:layout_width="30dp"
							android:layout_height="30dp"
							android:src="@drawable/search"
							android:scaleType="fitCenter" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/down"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/left"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal"
					android:orientation="vertical"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/bg_one"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginRight="10dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/img_one"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/one"
								android:layout_width="130dp"
								android:layout_height="130dp"
								android:src="@drawable/image"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear8"
							android:layout_width="match_parent"
							android:layout_height="56dp"
							android:background="#FFFFFF"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear9"
								android:layout_width="26dp"
								android:layout_height="26dp"
								android:layout_marginLeft="10dp"
								android:layout_marginTop="5dp"
								android:layout_marginRight="10dp"
								android:layout_marginBottom="5dp"
								android:padding="8dp"
								android:orientation="vertical" />
							<LinearLayout
								android:id="@+id/linear10"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear11"
									android:layout_width="80dp"
									android:layout_height="9dp"
									android:padding="8dp"
									android:orientation="horizontal" />
								<LinearLayout
									android:id="@+id/linear12"
									android:layout_width="50dp"
									android:layout_height="9dp"
									android:layout_marginTop="5dp"
									android:padding="8dp"
									android:orientation="horizontal" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear27"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="30dp"
						android:layout_marginRight="10dp"
						android:layout_marginBottom="10dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear28"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/imageview5"
								android:layout_width="130dp"
								android:layout_height="130dp"
								android:src="@drawable/img3"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear29"
							android:layout_width="match_parent"
							android:layout_height="56dp"
							android:background="#FFFFFF"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear30"
								android:layout_width="26dp"
								android:layout_height="26dp"
								android:layout_marginLeft="10dp"
								android:layout_marginTop="5dp"
								android:layout_marginRight="10dp"
								android:layout_marginBottom="5dp"
								android:padding="8dp"
								android:orientation="vertical" />
							<LinearLayout
								android:id="@+id/linear31"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear32"
									android:layout_width="80dp"
									android:layout_height="9dp"
									android:padding="8dp"
									android:orientation="horizontal" />
								<LinearLayout
									android:id="@+id/linear33"
									android:layout_width="50dp"
									android:layout_height="9dp"
									android:layout_marginTop="5dp"
									android:padding="8dp"
									android:orientation="horizontal" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal"
					android:orientation="vertical"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/linear20"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginRight="10dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear21"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/imageview4"
								android:layout_width="130dp"
								android:layout_height="130dp"
								android:src="@drawable/image2"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear22"
							android:layout_width="match_parent"
							android:layout_height="56dp"
							android:background="#FFFFFF"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear23"
								android:layout_width="26dp"
								android:layout_height="26dp"
								android:layout_marginLeft="10dp"
								android:layout_marginTop="5dp"
								android:layout_marginRight="10dp"
								android:layout_marginBottom="5dp"
								android:padding="8dp"
								android:orientation="vertical" />
							<LinearLayout
								android:id="@+id/linear24"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear25"
									android:layout_width="80dp"
									android:layout_height="9dp"
									android:padding="8dp"
									android:orientation="horizontal" />
								<LinearLayout
									android:id="@+id/linear26"
									android:layout_width="50dp"
									android:layout_height="9dp"
									android:layout_marginTop="5dp"
									android:padding="8dp"
									android:orientation="horizontal" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear34"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="30dp"
						android:layout_marginRight="10dp"
						android:layout_marginBottom="10dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear35"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/imageview6"
								android:layout_width="130dp"
								android:layout_height="130dp"
								android:src="@drawable/img4"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear36"
							android:layout_width="match_parent"
							android:layout_height="56dp"
							android:background="#FFFFFF"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear37"
								android:layout_width="26dp"
								android:layout_height="26dp"
								android:layout_marginLeft="10dp"
								android:layout_marginTop="5dp"
								android:layout_marginRight="10dp"
								android:layout_marginBottom="5dp"
								android:padding="8dp"
								android:orientation="vertical" />
							<LinearLayout
								android:id="@+id/linear38"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear39"
									android:layout_width="80dp"
									android:layout_height="9dp"
									android:padding="8dp"
									android:orientation="horizontal" />
								<LinearLayout
									android:id="@+id/linear40"
									android:layout_width="50dp"
									android:layout_height="9dp"
									android:layout_marginTop="5dp"
									android:padding="8dp"
									android:orientation="horizontal" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear54"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|bottom"
				android:orientation="horizontal"
				android:layout_weight="1">
				<TextView
					android:id="@+id/textview12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="\@All Right Reserved."
					android:textSize="12sp"
					android:textColor="#9E9E9E" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
